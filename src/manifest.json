{
  "manifest_version": 2,
  "name": "Skip Proofpoint Redirect",
  "version": "0.0.2",
  "description": "Skip Proofpoint URL Defense redirections",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://urldefense.proofpoint.com/v1/url?*",
    "https://urldefense.proofpoint.com/v2/url?*"
  ]
}
